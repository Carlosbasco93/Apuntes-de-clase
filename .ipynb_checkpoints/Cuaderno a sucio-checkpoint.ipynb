    "def doble(x):\n",
    "  return x * 2\n",
    "  \n",
    "mi_lista = [1, 2, 3, 4, 5, 6]\n",
    "lista_nueva = list(map(doble, mi_lista))\n",
    "print(lista_nueva) # [2, 4, 6, 8, 10, 12]\n",
    "\n",
    "#Ejemplo con lambda\n",
    "\n",
    "lista_nueva_lambda = list(map(lambda x: x * 2, mi_lista))\n",
    "print(lista_nueva_lambda)  # [2, 4, 6, 8, 10, 12]"
